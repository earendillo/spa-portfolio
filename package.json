{
    "name": "my-portfolio",
    "version": "1.0.0",
    "description": "portfolio created as single page app; built from angular-seed",
    "devDependencies": {
        "angular": "^1.6.5",
        "angular-route": "^1.6.5",
        "babel": "^6.23.0",
        "babel-core": "^6.25.0",
        "babel-loader": "^7.1.1",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "bower": "^1.7.7",
        "css-loader": "^0.28.4",
        "http-server": "^0.9.0",
        "less": "^2.7.2",
        "less-loader": "^4.0.5",
        "rimraf": "^2.6.1",
        "style-loader": "^0.18.2",
        "webpack": "^3.3.0",
        "webpack-dev-server": "^2.5.1"
    },
    "scripts": {
        "build": "rimraf app/dist && lessc app/style.less app/app.css && webpack",
        "style": "lessc app/style.less app/app.css",
        "dev": "webpack-dev-server",
        "postinstall": "bower install",
        "prestart": "npm install",
        "start": "http-server -a localhost -p 8000 -c-1 ./app"
    },
    "dependencies": {
        "reset-css": "^2.2.0"
    }
}
